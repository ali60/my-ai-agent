{
    "os": "windows",
    "region": "us-west-2",
    "default_model": "titan",
    "claude": {
        "modelId": "anthropic.claude-v2",
        "prompt_format": "\n\nHuman: {prompt}\n\nAssistant:",
        "max_tokens": 1000,
        "temperature": 0.7,
        "top_p": 1,
        "stop_sequences": ["\n\nHuman:"]
    },
    "llama": {
        "modelId": "meta.llama3-70b-instruct-v1:0",
        "prompt_format": "[INST] {prompt} [/INST]",
        "max_tokens": 4096,
        "temperature": 0.5,
        "top_p": 0.9
    },
    "titan": {
        "modelId": "amazon.titan-text-lite-v1",
        "prompt_format": "{prompt}",
        "max_tokens": 3072,
        "temperature": 0.7,
        "top_p": 1
    },
    "openai": {
        "modelId": "gpt-4",
        "max_tokens": 4096,
        "temperature": 0.7
    }
}
